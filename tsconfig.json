{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015"],
    "jsx": "react",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false, // Ensure noEmit is set to false
    "isolatedModules": false
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
